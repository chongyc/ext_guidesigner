  text : "Advanced",
  cls : "folder",
  childeren : [{
      text : "Grid",
      qtip : "A grid",
      config : {
        xtype : "grid",
        border : false,
        viewConfig : {
          forceFit : true
        },
        __JSON__ds : "new Ext.data.Store({reader: new Ext.data.ArrayReader({}, [{name: 'comment'}]),data: [['Please set CM and DS properties']]})",
